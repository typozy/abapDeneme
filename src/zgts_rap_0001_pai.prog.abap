MODULE user_command_0100 INPUT.
  CASE sy-ucomm.
    WHEN '&BACK'.
      SET SCREEN 0.
    WHEN '&EXIT'.
      SET SCREEN 0.
    WHEN '&CRE'.
      go_oo_alv->create_contract( ).
      go_oo_alv->refresh( ).
    WHEN OTHERS.
  ENDCASE.
ENDMODULE.
